
public class Vaixell {
	byte L;
	Posicions ps;
	
	public Vaixell(byte l){
		L=l;
		ps = new Posicions();
	}
	
	public Vaixell(byte l,Posicio...r){	
		L=l;
		ps = new Posicions(r);		
	}
	
	public String toString(){
		return "L="+L+":\n"+ps.toString();
	}
	
	public char cela(byte i,byte j){
		char R='Â·';
		
		if (ps.quantitat()>1) return R;
		
		return R;
	}
}
