		o = h;
	}
	
	public String toString(){
		return "("+I.x+","+I.y+","+(o?"-":"|")+")";
	}
	
	public boolean equals(Posicio r){
		return (I.x==r.I.x&&I.y==r.I.y&&o==r.o);
	}
}
