
public class Posicio {
	Index I
	boolean o;
	
	public Posicio(byte i, byte j, boolean h){
		I = new Index(i,j);
		o = h; 	
	}
	
	public Posicio(Index i,boolean h){
		I = i;
		o = h;
	}
	
	public String toString(){
		return "("+r.x+","+r.y+","+(o?"-":"|")+")";
	}
	
	public boolean equals(Posicio r){
		return (r.x==r.r.x&&r.y==r.r.y&&o==r.o);
	}
}
