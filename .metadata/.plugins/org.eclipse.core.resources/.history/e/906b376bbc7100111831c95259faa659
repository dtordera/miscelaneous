
public class Posicio extends Index {
	boolean o;
	
	public Posicio(byte i, byte j, boolean h){
		super (i,j);
		o = h; 	
	}
	
	public Posicio(Index i,boolean h){
		super(i);
		o = h;
	}
	
	private byte Px(){
		return o?1:0;
	} 
	
	public String toString(){
		return "("+x+","+y+","+(o?"-":"|")+")";
	}
	
	public boolean equals(Posicio r){
		return (x==r.x&&y==r.y&&o==r.o);
	}
	
	
}
