import java.util.Arrays;


public class Posicions {
	Posicio[] p;
	
	public Posicions(Posicio[] l){
		p = Arrays.copyOf(l,l.length);
	}
	
	public Posicions(){
		p = new Posicio[0];
	}
	
	public String toString(){
		String s;
		
		for (int n=0;n<p.length;n++)
		s += p[n].toString() + "\n";
		
		return s; 		
	}
}
