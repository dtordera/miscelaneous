import java.util.Arrays;


public class Puzzle {

	byte C,R;
	Vaixell[] V;
	Index[] DS;
	byte[] SR,SC;
	
	public Puzzle(byte c,byte r,byte[] sr,byte[] sc,byte[] l,Index[] i){
		C=c;
		R=r;
		SR = sr.clone();
		SC = sc.clone();
		
		DS = Arrays.copyOf(i,i.length);
		V = new Vaixell[l.length];
		
		for (int n=0;n<l.length;n++)	V[n] = new Vaixell(l[n]);
	}
	
	public String toString(){
		String s;
	}		
}
