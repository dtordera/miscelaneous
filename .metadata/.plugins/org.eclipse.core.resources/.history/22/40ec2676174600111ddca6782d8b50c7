
public class Yubotu01 {

	public static void main(String[] args) {
	
		int[] 		SR = {1,3,0,2,1,1,3,0,2,1};
		int[] 		SC = {2,1,2,1,1,1,3,0,2,1};
		cella[]	 	C = {	new cella(1,2,k.x_)};
		Vaixell[] 	V = { 	new Vaixell(0,0,k.CRE,k._v_),
							new Vaixell(0,0,k.DES,k._v_),
							new Vaixell(0,0,k.DES,k._v_),
							new Vaixell(0,0,k.DES,k._v_),														
							new Vaixell(0,0,k.SUB,k._h_),
							new Vaixell(0,0,k.SUB,k._h_)							new Vaixell(0,0,k.CRE,k._v_),
					};
				    
		Puzzle P = new Puzzle(10,10,V,C,SR,SC);

		P.Comproba();
		P.Resol();
	}

}
